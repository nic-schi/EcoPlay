<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StartseiteItemFragment">
        <attr name="image" format="reference" />
        <attr name="text" format="string" />
        <attr name="redirect" format="reference" />
    </declare-styleable>

    <declare-styleable name="FAQFragment">
        <attr name="collapsed" format="boolean" />
        <attr name="FAQFrage" format="string" />
        <attr name="antwort" format="string" />
    </declare-styleable>

    <declare-styleable name="SidekickOrHeroFragment">
        <attr name="frage" format="string" />
        <attr name="antwort_sidekick" format="string" />
        <attr name="antwort_held" format="string" />
    </declare-styleable>

    <declare-styleable name="StickerFragment">
        <attr name="stickername" format="string" />
        <attr name="description" format="string" />
        <attr name="stickericon" format="reference" />
    </declare-styleable>

    <declare-styleable name="InhalteFragment">
        <attr name="thema" format="string" />
        <attr name="inhaltIcon" format="reference" />
        <attr name="inhaltImage" format="reference" />
        <attr name="inhaltRedirect" format="string" />
    </declare-styleable>

    <declare-styleable name="HeaderFragment">
        <attr name="headertext" format="string" />
    </declare-styleable>

    <declare-styleable name="DragStickerFragment">
        <attr name="stickertag" format="string" />
    </declare-styleable>
</resources>